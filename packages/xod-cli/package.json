  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "dependencies": {
    "cli-color": "^1.1.0",
    "docopt": "^0.6.2",
    "hm-def": "^0.2.1",
    "inquirer": "^3.2.0",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "sanctuary-def": "^0.12.1",
    "source-map-support": "^0.4.15",
    "swagger-client": "^3.4.3",
    "xod-arduino": "^0.17.0",
    "xod-fs": "^0.17.0",
    "xod-func-tools": "^0.17.0",
    "xod-pm": "^0.17.0",
    "xod-project": "^0.17.0",
    "snyk": "^1.316.1"
